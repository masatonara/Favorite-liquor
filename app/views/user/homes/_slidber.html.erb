<script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>

<div id="multiple-items">
  <% @randam.each do |liquor| %>
   <div class="slide-item">
     <%= link_to liquor_path(liquor) do %>
      <%= attachment_image_tag liquor, :liquor_image, :fill, 300, 300, fallback: "no_image.jpeg", size: "300x300", class:"card-img-top";  %>
    <% end %>
  <% end %>
</div>